	public Bird (int x , int y , int w , int h ) {
		super (x,y,w,h) ; 
	}
	
	//update chim r∆°i 
	public void udate (long detaTime) {
		vt += Cau1_GameFlappyBird.g ; 
		this.setPosX(this.getPosY()+vt); 
		
		
	}



}
